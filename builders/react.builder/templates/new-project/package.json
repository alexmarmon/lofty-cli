{
  "name": "{{name}}",
  "version": "2.0.0",
  "description": "{{description}}",
  "author": "{{author}}",
  "license": "MIT",
  "repository": "{{repo}}",
  "framework": "{{framework}}",
  "scripts": {
    "dev": "cross-env PORT={{port}} node config/dev-server.js",
    "lint": "eslint --ext .js --ext .jsx src/; exit 0",
    "lint-fix": "eslint --fix --ext .js --ext .jsx src/; exit 0",
    "build": "cross-env PORT={{port}} webpack --config config/webpack.prod.js --progress --colors --optimize-minimize",
    "test": "cross-env PORT={{port}} node_modules/.bin/ava src/tests",
    "production": "cross-env PORT={{port}} node config/prod-server.js"
  },
  "keywords": [
    "react",
    "mobx",
    "template",
    "starter-kit",
    "react router 4",
    "webpack 3"
  ],
  "eslintConfig": {
    "extends": "airbnb",
    "parser": "babel-eslint",
    "parserOptions": {
      "sourceType": "module",
      "parser": "babel-eslint"
    },
    "globals": {
      "document": true,
      "fetch": true,
      "console": true,
      "it": true,
      "describe": true,
      "window": true
    },
    "rules": {
      "no-console": 0,
      "window": 0,
      "jsx-a11y/no-static-element-interactions": 0,
      "radix": 0,
      "forbid-prop-types": 0,
      "react/forbid-prop-types": 0,
      "no-nested-ternary": 0,
      "max-len": 0,
      "react/require-default-props": 0,
      "react/jsx-wrap-multilines": 0,
      "import/first": 0,
      "quote-props": 0,
      "radix": 0,
      "space-infix-ops": 0,
      "no-unused-vars": 0,
      "default-case": 0,
      "no-else-return": 0,
      "quotes": 0,
      "no-undef": 0,
      "react/prefer-stateless-function": 0,
      "react/no-unused-prop-types": 0,
      "no-useless-constructor": 0,
      "import/extensions": 0
    },
    "settings": {
      "import/resolver": {
        "webpack": {
          "config": "config/webpack.dev.js"
        }
      }
    }
  },
  "babel": {
    "presets": [
      "react",
      "stage-2",
      [
        "es2015",
        {
          "modules": false
        }
      ]
    ],
    "plugins": [
      "transform-decorators-legacy",
      "transform-class-properties",
      "transform-runtime"
    ]
  },
  "ava": {
    "require": [
      "babel-register",
      "ignore-styles",
      "./src/tests/helpers/setup.js"
    ],
    "babel": "inherit",
    "verbose": true
  },
  "dependencies": {
    "@lofty/lofty-pretty-print-plugin": "^1.0.1",
    "babel-eslint": "^8.0.0",
    "babel-loader": "^7.1.2",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.7.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.3.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "ignore-styles": "^5.0.1",
    "postcss-loader": "^2.0.6",
    "react-addons-test-utils": "^15.6.0",
    "react-test-renderer": "^15.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "ava": "^0.22.0",
    "body-parser": "^1.18.1",
    "chokidar": "^1.7.0",
    "compression": "^1.7.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.5",
    "enzyme": "^2.9.1",
    "eslint-plugin-import": "^2.7.0",
    "express": "^4.15.4",
    "html-webpack-plugin": "^2.30.1",
    "jsdom": "^11.2.0",
    "knex": "^0.13.0",
    "mobx": "^3.3.0",
    "mobx-react": "^4.3.2",
    "mysql": "^2.14.1",
    "node-sass": "^4.5.3",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "sitemap": "^1.13.0",
    "supertest": "^3.0.0",
    "velocity-react": "^1.3.3",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.8.2",
    "webpack-hot-middleware": "^2.19.1",
    "webpack-merge": "^4.1.0",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-register": "^6.26.0"
  }
}
