{
  "parser": "babel-eslint",
  "extends": "airbnb-base",
  "globals": {
    "console": true
  },
  "rules": {
    "no-console": 0,
    "window": 0,
    "forbid-prop-types": 0,
    "no-nested-ternary": 0,
    "max-len": 0,
    "import/first": 0,
    "quote-props": 0,
    "radix": 0,
    "space-infix-ops": 0,
    "default-case": 0,
    "no-else-return": 0,
    "quotes": 0,
    "no-undef": 0,
    "no-useless-constructor": 0,
    "import/extensions": 0,
    "brace-style": 0,
    "prefer-promise-reject-errors": 0,
    "no-param-reassign": 0,
    "prefer-destructuring": 0
  },
}
